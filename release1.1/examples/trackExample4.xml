<?xml version="1.0" encoding="UTF-8"?>
<bxExamples
	xmlns="http://bioxsd.org/example"
	xmlns:bx="http://bioxsd.org/BioXSD-1.1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://bioxsd.org/example http://bioxsd.org/bxExamples(1.1).xsd"
>
	<exampleTrack>
		<bx:referenceSequence>
			<bx:species/>
			<bx:genomeBuild/>
			<bx:chromosome>1</bx:chromosome>
			<bx:subsequencePosition>
				<bx:segment min="201" max="600"/>
			</bx:subsequencePosition>
		</bx:referenceSequence>
		<bx:annotation>
			<bx:feature>
				<bx:name>Example track 4</bx:name>
			</bx:feature>
			<bx:occurrence>
				<bx:appliesToWholeSequence/>
				<bx:score value="25">
					<bx:position>
						<bx:nextPartition max="100"/>
					</bx:position>
				</bx:score>
				<bx:score value="26">
					<bx:position>
						<bx:nextPartition max="200"/>
					</bx:position>
				</bx:score>
				<bx:score value="25">
					<bx:position>
						<bx:nextPartition max="300"/>
					</bx:position>
				</bx:score>
				<bx:score value="23">
					<bx:position>
						<bx:nextPartition max="400"/>
					</bx:position>
				</bx:score>
			</bx:occurrence>
		</bx:annotation>
	</exampleTrack>
	<exampleTrack>
		<bx:referenceSequence>
			<bx:species/>
			<bx:genomeBuild/>
			<bx:chromosome>2</bx:chromosome>
			<bx:subsequencePosition>
				<bx:segment min="151" max="550"/>
			</bx:subsequencePosition>
		</bx:referenceSequence>
		<bx:annotation>
			<bx:feature>
				<bx:name>Example track 4</bx:name>
			</bx:feature>
			<bx:occurrence>
				<bx:appliesToWholeSequence/>
				<bx:score value="10">
					<bx:position>
						<bx:nextPartition max="100"/>
					</bx:position>
				</bx:score>
				<bx:score value="11">
					<bx:position>
						<bx:nextPartition max="200"/>
					</bx:position>
				</bx:score>
				<bx:score value="13">
					<bx:position>
						<bx:nextPartition max="300"/>
					</bx:position>
				</bx:score>
				<bx:score value="14">
					<bx:position>
						<bx:nextPartition max="400"/>
					</bx:position>
				</bx:score>
			</bx:occurrence>
		</bx:annotation>
	</exampleTrack>
</bxExamples>